---
import { Image } from 'astro:assets';

interface Props {
  src: string;
  alt: string;
  width?: number;
  height?: number;
  className?: string;
  loading?: 'eager' | 'lazy';
}

const props = Astro.props;
---

<Image referrerpolicy="no-referrer" class="w-full rounded-xl" width={800} height={500} {...props} />
